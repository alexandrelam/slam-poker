{
  "id": null,
  "title": "SLAM Poker - Performance",
  "tags": ["slam-poker", "performance", "latency"],
  "style": "dark",
  "timezone": "browser",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "templating": {
    "list": [
      {
        "name": "instance",
        "type": "query",
        "query": "label_values(slam_poker_http_requests_total, instance)",
        "current": {
          "selected": false,
          "text": "All",
          "value": "$__all"
        },
        "includeAll": true,
        "allValue": ".*",
        "refresh": "time"
      }
    ]
  },
  "graphTooltip": 0,
  "hideControls": false,
  "links": [
    {
      "title": "Overview",
      "tooltip": "Main KPI dashboard",
      "type": "dashboard",
      "tags": ["slam-poker", "overview"],
      "targetBlank": false
    },
    {
      "title": "System Health",
      "tooltip": "Infrastructure and Node.js metrics",
      "type": "dashboard",
      "tags": ["slam-poker", "system"],
      "targetBlank": false
    },
    {
      "title": "Room Analytics",
      "tooltip": "View detailed room and voting analytics",
      "type": "dashboard",
      "tags": ["slam-poker", "rooms"],
      "targetBlank": false
    },
    {
      "title": "Alerts & SLA",
      "tooltip": "SLA compliance and alert status",
      "type": "dashboard",
      "tags": ["slam-poker", "alerts"],
      "targetBlank": false
    }
  ],
  "liveNow": false,
  "panels": [
    {
      "id": 1,
      "title": "HTTP Request Duration - Percentiles",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(slam_poker_http_request_duration_seconds_bucket[5m]))",
          "legendFormat": "50th percentile",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.90, rate(slam_poker_http_request_duration_seconds_bucket[5m]))",
          "legendFormat": "90th percentile",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.95, rate(slam_poker_http_request_duration_seconds_bucket[5m]))",
          "legendFormat": "95th percentile",
          "refId": "C"
        },
        {
          "expr": "histogram_quantile(0.99, rate(slam_poker_http_request_duration_seconds_bucket[5m]))",
          "legendFormat": "99th percentile",
          "refId": "D"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "s",
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.1 },
              { "color": "red", "value": 0.5 }
            ]
          }
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 }
    },
    {
      "id": 2,
      "title": "WebSocket Event Duration - Percentiles",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(slam_poker_websocket_event_duration_seconds_bucket[5m]))",
          "legendFormat": "50th percentile",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.90, rate(slam_poker_websocket_event_duration_seconds_bucket[5m]))",
          "legendFormat": "90th percentile",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.95, rate(slam_poker_websocket_event_duration_seconds_bucket[5m]))",
          "legendFormat": "95th percentile",
          "refId": "C"
        },
        {
          "expr": "histogram_quantile(0.99, rate(slam_poker_websocket_event_duration_seconds_bucket[5m]))",
          "legendFormat": "99th percentile",
          "refId": "D"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "s",
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.05 },
              { "color": "red", "value": 0.1 }
            ]
          }
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 }
    },
    {
      "id": 3,
      "title": "Session Duration Percentiles",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(slam_poker_session_duration_seconds_bucket[5m]))",
          "legendFormat": "50th percentile",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.90, rate(slam_poker_session_duration_seconds_bucket[5m]))",
          "legendFormat": "90th percentile",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.95, rate(slam_poker_session_duration_seconds_bucket[5m]))",
          "legendFormat": "95th percentile",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "s"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 }
    },
    {
      "id": 4,
      "title": "Request Rate by Endpoint",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(slam_poker_http_requests_total[5m])",
          "legendFormat": "{{method}} {{path}} ({{status_code}})",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "reqps"
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 }
    },
    {
      "id": 5,
      "title": "WebSocket Event Rate by Type",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(slam_poker_websocket_event_duration_seconds_count[5m])",
          "legendFormat": "{{event_type}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "ops"
        }
      },
      "gridPos": { "h": 6, "w": 24, "x": 0, "y": 16 }
    }
  ],
  "refresh": "30s",
  "schemaVersion": 27,
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "",
  "version": 1
}
